{
	"extends": "stylelint-config-recommended-less",
	"rules": {
		"block-no-empty": null,
		"no-extra-semicolons": null
	},
	"ignoreFiles": [
		"build/*",
		"dist/",
		"src/assets/*",
		"**/*.ts",
		"**/*.png",
		"**/*.js"
	],
	"overrides": [
    {
      "files": ["*.less"],
      "customSyntax": "postcss-less"
    }
  ]
}
